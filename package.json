    "build-docs": " $(yarn bin)/documentation build --config docs/documentation.yml --access public --github --format md --output docs/api.md index.js lib/**"
  },
  "engines": {
    "node": "6.x.x"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mapbox/carmen.git"
  }
